 else {\r\n      currentValues.value[index] = endValue\r\n    }\r\n  }\r\n  counter()\r\n}\r\n\r\nonMounted(() => {\r\n  counters.value.forEach((item, index) => {\r\n    animateCounter(index, item.value)\r\n  })\r\n})\r\n\r\nconst displayValue = (value, index) => currentValues.value[index]\r\n</script>\r\n\r\n<style scoped>\r\nsection {\r\n  position: relative;\r\n  z-index: 10;\r\n}\r\n\r\n.container {\r\n  max-width: 1200px;\r\n}\r\n.parallax-bg {\r\n  background-attachment: fixed;\r\n  background-size: cover;\r\n  background-position: center;\r\n}\r\n</style>\r\n","<script setup lang=\"ts\">\r\nimport Breadcrumbs from '~/components/templates/breadcrumbs.vue';\r\n</script>\r\n\r\n<template>\r\n       <section class=\"relative w-full h-[100vh] overflow-hidden flex items-center justify-center mavrix-hero-section\">\r\n        <!-- <img src=\"/main_banner.jpeg\" alt=\"\" srcset=\"\" class=\"absolute inset-0 w-full h-full object-cover top-center\"/> -->\r\n\r\n    <!-- ðŸ–¼ï¸ Optional Dark Overlay -->\r\n    <div class=\"absolute inset-0 bg-black/50 backdrop-blur-[1px]\"></div>\r\n            <div class=\"relative z-10 w-full px-6 md:px-12 lg:px-20\">\r\n  <UPageHero\r\n  class=\"transition-all\"\r\n  title=\"OUR SERVICES\"\r\n  description=\"Find handpicked villas, apartments, and townhouses across Dubaiâ€™s prime communities â€” Downtown Dubai, Dubai Hills, Palm Jumeirah, and more.\"\r\n  \r\n  />\r\n    <breadcrumbs />\r\n<div class=\"flex justify-center mt-8\">\r\n        <NuxtLink\r\n          to=\"#next-section\"\r\n          \r\n          class=\"block hover:opacity-80 transition-opacity duration-300\"\r\n        >\r\n          <img\r\n            src=\"/Scroll.gif\"\r\n            alt=\"Scroll Down\"\r\n            class=\"w-[60px] md:w-[80px] object-contain\"\r\n            loading=\"lazy\"\r\n          />\r\n        </NuxtLink>\r\n      </div>\r\n</div>\r\n</section>\r\n</template>\r\n","import { publicAssetsURL } from '#internal/nuxt/paths';export default p